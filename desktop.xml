        <Modifiers>
          <Modifier>Control</Modifier>
        </Modifiers>
        <Key>20</Key>
      </KeyStroke>
    </MacroEntry>
    <MacroEntry>
      <ID>CTRL+4</ID>
      <KeyStroke>
        <Modifiers>
          <Modifier>Control</Modifier>
        </Modifiers>
        <Key>21</Key>
      </KeyStroke>
    </MacroEntry>

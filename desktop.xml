          </Input>
          <Output>
            <ActionID>CTRL+3</ActionID>
          </Output>
        </Button>
        <Button>
          <Input>
            <ActionID>HIDButton_16</ActionID>
          </Input>
          <Output>
            <ActionID>CTRL+4</ActionID>
          </Output>
        </Button>
      </ButtonBank>
      <!-- Volume up -->
      <Axis>
        <Enabled>true</Enabled>
        <Input>
          <ActionID>HIDMultiAxis_Rz</ActionID>
